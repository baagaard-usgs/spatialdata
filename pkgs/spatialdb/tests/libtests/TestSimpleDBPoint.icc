// Get data for database.
inline
const double*
spatialdata::TestSimpleDBPoint::Data(void) const
{ return DATA; }

// Get names of values in database.
inline
const char**
spatialdata::TestSimpleDBPoint::Names(void) const
{ return NAMES; }

// Get number of locations in database.
inline
int 
spatialdata::TestSimpleDBPoint::NumLocs(void) const
{ return NUMLOCS; }

// Get number of values at each location in database.
inline
int
spatialdata::TestSimpleDBPoint::NumVals(void) const
{ return NUMVALS; }

// Get topology of data in database.
inline
spatialdata::SimpleDB::TopoEnum
spatialdata::TestSimpleDBPoint::Topology(void) const
{ return TOPOLOGY; }

// Get number of queries.
inline
int
spatialdata::TestSimpleDBPoint::NumQueries(void) const
{ return NUMQUERIES; }

// Get query data for nearest neighbor algorithm.
inline
const double*
spatialdata::TestSimpleDBPoint::QueryNearest(void) const
{ return QUERYNEAREST; }

// Get query data for linear interpolation algorithm.
inline
const double*
spatialdata::TestSimpleDBPoint::QueryLinear(void) const
{ return QUERYLINEAR; }

